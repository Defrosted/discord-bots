        "dotenv": "^16.0.3",
        "eslint": "^8.25.0",
        "jest": "^29.2.1",
        "resolve-tspaths": "^0.8.1",
        "ts-jest": "^29.0.3",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "aws-sdk": "^2.1235.0",
